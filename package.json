{
  "name": "eleventy_solo_starter",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site src/_includes/css",
    "devsass": "sass --no-source-map src/assets/scss/index.scss _site/css/index.css",
    "prodsass": "sass --style=compressed --no-source-map src/assets/scss/index.scss src/_includes/css/index.css",
    "devpostcss": "postcss _site/css/index.css -o _site/css/index.css",
    "prodpostcss": "postcss src/_includes/css/index.css -o src/_includes/css/index.css",
    "imgs": "node imgxfm.js",
    "start": "NODE_ENV=development npm-run-all clean devsass imgs --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean prodsass imgs prod:sass prod:prefix prod:eleventy",
    "testbuild": "NODE_ENV=production npm-run-all clean prodsass imgs --parallel testProd:*",
    "dev:sass": "npm run devsass --watch",
    "dev:prefix": "npm run devpostcss --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --quiet --serve",
    "prod:sass": "npm run prodsass",
    "prod:prefix": "npm run prodpostcss",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "testProd:sass": "npm run prodsass --watch",
    "testProd:prefix": "npm run prodpostcss --watch",
    "testProd:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site --quiet --serve"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "> 1%",
    "IE 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "autoprefixer": "^10.2.5",
    "babel-core": "^6.26.3",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-env": "^1.7.0",
    "debug": "^4.3.1",
    "del": "^6.0.0",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "fibers": "^5.0.0",
    "get-json": "^1.0.1",
    "glob": "^7.1.6",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.7",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.2",
    "inline-source-map": "^0.6.2",
    "jsonfile": "^6.1.0",
    "junk": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.1.6",
    "md5": "^2.3.0",
    "node-dir": "^0.1.17",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.15.1",
    "prismjs": "^1.23.0",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.3.3",
    "sass": "^1.32.8",
    "sharp": "^0.28.0"
  }
}
