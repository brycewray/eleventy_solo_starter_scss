{
  "name": "eleventy_solo_starter",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site src/_includes/css",
    "devsass": "sass --no-source-map src/assets/scss/index.scss _site/css/index.css",
    "prodsass": "sass --no-source-map src/assets/scss/index.scss src/_includes/css/index.css",
    "devpostcss": "postcss _site/css/index.css -o _site/css/index.css",
    "prodpostcss": "postcss src/_includes/css/index.css -o src/_includes/css/index.css",
    "start": "NODE_ENV=development npm-run-all clean devsass --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean prodsass prod:sass prod:prefix prod:eleventy",
    "testbuild": "NODE_ENV=production npm-run-all clean prodsass --parallel testProd:*",
    "dev:sass": "npm run devsass -- --watch",
    "dev:prefix": "npm run devpostcss --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --quiet --serve",
    "prod:sass": "npm run prodsass",
    "prod:prefix": "npm run prodpostcss",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "testProd:sass": "npm run prodsass -- --watch",
    "testProd:prefix": "npm run prodpostcss --watch",
    "testProd:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site --quiet --serve"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "> 1%",
    "IE 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "autoprefixer": "^10.2.5",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "glob": "^7.1.7",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "luxon": "^1.27.0",
    "markdown-it": "^12.0.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "postcss-csso": "^5.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.13"
  }
}
